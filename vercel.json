{
  "version": 0.6,
  "name": "pink",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/how-to-use",
      "destination": "/how-to-use.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  }
}
